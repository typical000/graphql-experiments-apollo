{
  "parser": "babel-eslint",
  "extends": "eslint-config-jss",
  "plugins": [
    "babel"
  ],
  "rules": {
    "import/no-unresolved": [2, {
      "ignore": [
        "react",
        "react-dom"
      ]
    }],
    "object-curly-spacing": "off",
    "no-underscore-dangle": "off",
    "babel/object-curly-spacing": 2,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "jest": true
  }
}
