{
  "name": "qraphql-experiments",
  "private": true,
  "version": "0.1.0",
  "description": "Experimenting with frontend (Relay Modern, Apollo)",
  "author": "Pavel Davydov",
  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=4000 node ./bin/webpack-dev-server.js",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.prod",
    "build:analyze": "cross-env NODE_ENV=production webpack --config ./webpack.config.analyze",
    "build:html:dev": "cross-env NODE_ENV=development node ./bin/build-html",
    "build:html:prod": "cross-env NODE_ENV=production node ./bin/build-html",
    "clean": "rimraf 'dist/*'",
    "all:dev": "npm run clean && npm run build:dev && npm run build:html:dev",
    "all:prod": "npm run clean && npm run build:prod && npm run build:html:prod",
    "precommit": "lint-staged",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "common-tags": "^1.5.1",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-jss": "^3.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-minifier": "^3.5.7",
    "husky": "^0.14.3",
    "ignore-styles": "^5.0.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^6.0.0",
    "module-alias": "^2.0.3",
    "rimraf": "^2.6.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7",
    "webpack-manifest-plugin": "^1.3.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "css-functions": "^1.0.5",
    "jss": "^9.3.3",
    "jss-preset-default": "^4.0.1",
    "normalize-jss": "^4.0.0",
    "polished": "^1.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-jss": "^8.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  },
  "license": "MIT"
}
